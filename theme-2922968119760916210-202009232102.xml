          <b:widget-settings>
            <b:widget-setting name='pageListJson'><![CDATA[{'4030929279921280607': {'href': 'https://quiznlearn.blogspot.com/p/learn.html', 'title': 'Learn', 'position': 0}, '2399261909046693114': {'href': 'https://quiznlearn.blogspot.com/p/practice.html', 'title': 'Practice', 'position': 1}, '8997173102473424837': {'href': 'https://quiznlearn.blogspot.com/p/test.html', 'title': 'Test', 'position': 2}}]]></b:widget-setting>
            <b:widget-setting name='homeTitle'>Home</b:widget-setting>
